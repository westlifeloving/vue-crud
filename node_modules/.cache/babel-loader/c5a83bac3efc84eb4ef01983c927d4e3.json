{"ast":null,"code":"export default {\n  data() {\n    return {\n      tableData: [{\n        name: \"示例规则_账号爆破防护\",\n        condition: \"URI 包含 /login\",\n        action: \"阻断\",\n        port: \"全部\",\n        priority: \"1\",\n        switchvalue: true\n      }, {\n        name: \"示例规则_API防护\",\n        condition: \"URI 包含 /api/\",\n        action: \"阻断\",\n        port: \"全部\",\n        priority: \"2\"\n      }, {\n        name: \"示例规则_网络爬虫放行\",\n        condition: \"User-Agent 包含 spider\",\n        action: \"放行\",\n        port: \"全部\",\n        priority: \"3\"\n      }],\n      multipleSelection: [],\n      dialogTableVisible: false,\n      dialogFormVisible: false,\n      form: {\n        name: \"\",\n        condition: \"\",\n        type: \"\",\n        action: \"\",\n        port: \"\",\n        priority: \"\",\n        switchvalue: false\n      },\n      ruleForm: {\n        name: \"\",\n        condition: \"\",\n        action: \"\",\n        port: \"\",\n        priority: \"\",\n        switchvalue: false\n      },\n      formLabelWidth: \"120px\",\n      rules: {\n        name: [{\n          required: true,\n          message: \"请输入策略名称\",\n          trigger: \"blur\"\n        }],\n        action: [{\n          required: true,\n          message: \"请选择匹配动作\",\n          trigger: \"change\"\n        }],\n        priority: [{\n          required: true,\n          message: \"请输入优先级\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n\n  methods: {\n    toggleSelection(rows) {\n      if (rows) {\n        rows.forEach(row => {\n          this.$refs.multipleTable.toggleRowSelection(row);\n        });\n      } else {\n        this.$refs.multipleTable.clearSelection();\n      }\n    },\n\n    handleChange(value) {\n      console.log(value);\n    },\n\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n\n    handleEdit(index, row) {\n      console.log(index, row);\n    },\n\n    deleteRow(index, rows) {\n      rows.splice(index, 1);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmZA;EACAA;IACA;MACAC,YACA;QACAC,mBADA;QAEAC,0BAFA;QAGAC,YAHA;QAIAC,UAJA;QAKAC,aALA;QAMAC;MANA,CADA,EASA;QACAL,kBADA;QAEAC,yBAFA;QAGAC,YAHA;QAIAC,UAJA;QAKAC;MALA,CATA,EAgBA;QACAJ,mBADA;QAEAC,iCAFA;QAGAC,YAHA;QAIAC,UAJA;QAKAC;MALA,CAhBA,CADA;MAyBAE,qBAzBA;MA0BAC,yBA1BA;MA2BAC,wBA3BA;MA4BAC;QACAT,QADA;QAEAC,aAFA;QAGAS,QAHA;QAIAR,UAJA;QAKAC,QALA;QAMAC,YANA;QAOAC;MAPA,CA5BA;MAqCAM;QACAX,QADA;QAEAC,aAFA;QAGAC,UAHA;QAIAC,QAJA;QAKAC,YALA;QAMAC;MANA,CArCA;MA6CAO,uBA7CA;MA8CAC;QACAb;UAAAc;UAAAC;UAAAC;QAAA,EADA;QAEAd,SACA;UAAAY;UAAAC;UAAAC;QAAA,CADA,CAFA;QAKAZ,WACA;UAAAU;UAAAC;UAAAC;QAAA,CADA;MALA;IA9CA;EAwDA,CA1DA;;EA4DAC;IACAC;MACA;QACAC;UACA;QACA,CAFA;MAGA,CAJA,MAIA;QACA;MACA;IACA,CATA;;IAUAC;MACAC;IACA,CAZA;;IAaAC;MACA;IACA,CAfA;;IAgBAC;MACAF;IACA,CAlBA;;IAmBAG;MACAL;IACA;;EArBA;AA5DA","names":["data","tableData","name","condition","action","port","priority","switchvalue","multipleSelection","dialogTableVisible","dialogFormVisible","form","type","ruleForm","formLabelWidth","rules","required","message","trigger","methods","toggleSelection","rows","handleChange","console","handleSelectionChange","handleEdit","deleteRow"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <el-container style=\"height: 100%; border: 1px solid #eee\">\n    <el-aside width=\"200px\" style=\"background-color: rgb(238, 241, 246)\">\n      <el-menu\n        :default-openeds=\"[]\"\n        background-color=\"#545c64\"\n        text-color=\"#fff\"\n      >\n        <el-submenu index=\"1\">\n          <template slot=\"title\"><i></i>域名管理</template>\n          <el-menu-item index=\"1-1\">服务概览</el-menu-item>\n          <el-menu-item index=\"1-2\">域名列表</el-menu-item>\n          <el-menu-item index=\"1-3\">流量记录</el-menu-item>\n          <el-menu-item index=\"1-4\">通用设置</el-menu-item>\n        </el-submenu>\n        <el-submenu index=\"2\">\n          <template slot=\"title\"><i></i>抗D保</template>\n          <el-men